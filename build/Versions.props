<Project>
  <PropertyGroup>
    <VersionBase>2.6.0</VersionBase>
    <PreReleaseVersionLabel>beta1</PreReleaseVersionLabel>

    <!-- Opt-in RepoToolset tools -->
    <UsingToolVSSDK>true</UsingToolVSSDK>

    <!-- Toolset -->
    <RoslynToolsMicrosoftRepoToolsetVersion>1.0.0-alpha54</RoslynToolsMicrosoftRepoToolsetVersion>
    <VSWhereVersion>2.1.4</VSWhereVersion>
    <MicrosoftVSSDKVersion>15.0.26201-alpha</MicrosoftVSSDKVersion>
    <Pdb2PdbVersion>1.1.0-beta1-62309-01</Pdb2PdbVersion>

    <!-- Roslyn -->
    <MicrosoftCodeAnalysisVersion>2.6.0-beta3-62309-01</MicrosoftCodeAnalysisVersion>
    <MicrosoftCodeAnalysisTestResourcesProprietaryVersion>2.0.0-pre-20160714</MicrosoftCodeAnalysisTestResourcesProprietaryVersion>
    <MicrosoftNetCompilersVersion>2.6.0-beta3-62309-01</MicrosoftNetCompilersVersion>

    <!-- CoreFX -->
    <SystemCollectionsImmutableVersion>1.3.1</SystemCollectionsImmutableVersion>
    <SystemReflectionMetadataVersion>1.4.2</SystemReflectionMetadataVersion>
    <MicrosoftVisualBasicVersion>10.1.0</MicrosoftVisualBasicVersion>
  </PropertyGroup>

  <PropertyGroup>
    <RestoreSources>
      $(RestoreSources);
      https://dotnet.myget.org/F/roslyn/api/v3/index.json;
      https://dotnet.myget.org/F/roslyn-tools/api/v3/index.json;
      https://dotnet.myget.org/F/roslyn-analyzers/api/v3/index.json
    </RestoreSources>
  </PropertyGroup>
</Project>

<configuration>
  <packageRestore>
    <!-- Currently, doing a restore from VS causes NuGet to stackoverflow. (https://github.com/NuGet/Home/issues/2558)  -->
    <add key="automatic" value="false" />
  </packageRestore>
  <solution>
    <add key="disableSourceControlIntegration" value="true" />
  </solution>
  <packageSources>
    <add key="nuget.org" value="https://www.nuget.org/api/v2/" />
    <add key="MyGet Roslyn" value="https://dotnet.myget.org/F/roslyn-master-nightly/api/v3/index.json" />
    <add key="MyGet CoreFX" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>

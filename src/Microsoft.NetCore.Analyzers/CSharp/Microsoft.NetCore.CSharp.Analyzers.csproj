  <ItemGroup>
    <ProjectReference Include="..\Core\Microsoft.NetCore.Analyzers.Core.csproj" />
  </ItemGroup>
  <ImportGroup Label="Targets">
    <Import Project="..\..\..\build\Targets\Analyzers.Imports.targets" />
  </ImportGroup>
</Project>